#include <stdio.h>
void swap(int *x, int *y){
    int temp;
    temp = *x;
    *x = *y;
    *y = temp;
    
    return;
}

int main() {
    int a,b;
    printf("Enter A : ");
    scanf("%d", &a);
    
    printf("Enter B : ");
    scanf("%d", &b);
    
    swap(&a, &b);
    printf("A = %d\nB = %d",a,b);

    return 0;
}
